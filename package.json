{
  "name": "firebase-backend",
  "version": "0.2.2",
  "description": "A package that helps with the management and expansion of a maintainable firebase backend",
  "author": "FilledStacks <dane@filledstacks.com>",
  "license": "MIT",
  "homepage": "https://github.com/filledstacks/firebase-backend#readme",
  "repository": "git://github.com/filledstacks/firebase-backend.git",
  "bugs": {
    "url": "git+https://github.com/filledstacks/firebase-backend/issues"
  },
  "main": "dist/index.js",
  "module": "dist/firebase-backend.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint src test",
    "lint:fix": "tsdx lint src test --fix",
    "prepare": "tsdx build",
    "release": "auto shipit"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/glob": "^7.1.3",
    "eslint": "^6.8.0",
    "husky": "^6.0.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/cors": "^2.8.10",
    "@types/express-fileupload": "^1.1.6",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "glob": "^7.1.7",
    "prettier": "^2.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": true,
    "quoteProps": "consistent"
  }
}
